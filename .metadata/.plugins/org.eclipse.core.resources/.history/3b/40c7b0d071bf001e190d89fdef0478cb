
package fr.greta.tp;

public class Factorielle {
    public static void main(String[] args) {
    	calculerFactorielle(5);
     
    }

  
    private static long calculerFactorielle(int n) {
        if (n == 0 || n == 1) {
            return 1;
        }
        long result = 1;
        for (int i = 2; i <= n; i++) {
            result *= i;
        }
        return result;
    }
}
