package fr.greta.tp;

public class NombresPremiers {
    public static void main(String[] args) {
      
        int count = 0;
        for (int num = 2; count < 10; num++) {
            if (estPremier(num)) {
                System.out.println(num);
                count++;
            }
        }
    }


    private static boolean estPremier(int n) {
        if (n <= 1) {
            return false;
        }
        for (int i = 2; i <= Math.sqrt(n); i++) {
            if (n % i == 0) {
                return false;
            }
        }
        return true;
    }
}
